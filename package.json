{
  "name": "@passflow/react",
  "description": "Passflow React UI Kit",
  "version": "0.1.42",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/index.es.js"
      },
      "require": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format src",
    "format:fix": "biome format --write src",
    "preview": "vite preview",
    "test": "pnpm playwright test",
    "test:ui": "pnpm playwright test --ui",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@passflow/core": "workspace:*",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "clsx": "^2.1.1",
    "countries-and-timezones": "^3.8.0",
    "dayjs": "^1.11.19",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "phone": "^3.1.69",
    "query-string": "^9.3.1",
    "react-error-boundary": "^4.1.2",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.71.1",
    "react-international-phone": "^4.6.1",
    "react-otp-input": "^3.1.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-scoped-preflight": "^3.5.7",
    "terser": "^5.44.1",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.23",
    "@types/node": "^22.19.6",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^4.0.17",
    "autoprefixer": "^10.4.23",
    "cssnano": "^7.1.2",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "6.30.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "5.8.2",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-environment": "^1.1.3",
    "vitest": "^4.0.17"
  },
  "authors": [
    {
      "name": "Ivan Holiak",
      "url": "https://github.com/IvanHoliak"
    },
    {
      "name": "Jack Rudenko",
      "url": "https://madappgang.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MadAppGang/passflow-react-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadAppGang/passflow-react-sdk.git",
    "directory": "packages/passflow-react-sdk"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "passflow",
    "auth",
    "authentication",
    "passwordless",
    "session",
    "jwt",
    "react",
    "ui",
    "ui-kit",
    "react-ui-kit",
    "passflow-react-ui-kit"
  ],
  "packageManager": "pnpm@10.8.0"
}
