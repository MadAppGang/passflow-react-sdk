  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MadAppGang/aooth-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadAppGang/aooth-js.git",
    "directory": "packages/react"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "aooth",
    "auth",
    "authentication",
    "passwordless",
    "session",
    "jwt",
    "react",
    "ui",
    "ui-kit",
    "react-ui-kit",
    "aooth-react-ui-kit"
  ]
