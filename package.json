  "version": "0.1.39",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/index.es.js"
      },
      "require": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    },
    "./dist/style.css": "./dist/style.css",
